{
  "model": {
    "name": "qwen3-vl-8b-ctx32k-explicit:latest",
    "description": "Qwen3-VL 8B model optimized for RTX 4090 (32K context)",
    "max_context_length": 32000,
    "parameters": {
      "max_new_tokens": 3000,
      "temperature": 0.7,
      "top_p": 0.8,
      "top_k": 20,
      "repeat_penalty": 1.5
    }
  },
  "video_analysis": {
    "frame_interval": 30,
    "description_fields": [
      "people",
      "environment",
      "actions",
      "camera_style",
      "camera_movement"
    ],
    "output_format": "json"
  },
  "system": {
    "required_packages": [
      "torch",
      "transformers",
      "opencv-python",
      "pillow",
      "numpy",
      "tqdm"
    ],
    "recommended_hardware": {
      "gpu": "CUDA-compatible GPU",
      "memory": "16GB RAM minimum",
      "storage": "Sufficient space for video files and model"
    }
  }
}